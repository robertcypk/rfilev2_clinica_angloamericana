<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\TblHorarios" table="tbl_horarios">
    <id name="idhorario" type="integer" column="idhorario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dias" type="string" column="dias" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="inicio" type="string" column="inicio" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fin" type="string" column="fin" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
