<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\TblDocumentos" table="tbl_documentos">
    <id name="iddoc" type="integer" column="iddoc">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idsolicitud" type="integer" column="idsolicitud" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hospitalizacion" type="text" column="hospitalizacion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="documento" type="text" column="documento" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="text" column="tipo" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
